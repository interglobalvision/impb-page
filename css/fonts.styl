@font-face {
  font-family: 'HK Grotesk Alternate';
  src: url('HKGrotesk-Alt-Light.eot');
  src: url('HKGrotesk-Alt-Light.eot?#iefix') format('embedded-opentype'),
    url('HKGrotesk-Alt-Light.woff2') format('woff2'),
    url('HKGrotesk-Alt-Light.woff') format('woff'),
    url('HKGrotesk-Alt-Light.ttf') format('truetype'),
    url('HKGrotesk-Alt-Light.svg#HKGrotesk-Alt-Light') format('svg');
  font-weight: normal;
  font-style: normal;
}

